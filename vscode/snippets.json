{
	"IIFE": {
		"prefix": "iife",
		"body": [
			"useEffect(() => {",
			"(async () => {",
			"// code",
			"})();",
			"});"
		],
		"description": ""
	},
	"Code block separator": {
		"prefix": "comm",
		"body": [
			"// ==========================================================================================",
			"// section name",
			"// ==========================================================================================",
			""
		],
		"description": ""
	},
	"No focus": {
		"prefix": "nofocus",
		"body": [
			"textarea:focus, input:focus, select:focus {",
			"  box-shadow: 0 0 0 0;",
			"  border: 0 none;",
			"  outline: 0;",
			"} "
		],
		"description": "No focus"
	},
	"Debug logger": {
		"prefix": "logger",
		"body": [
			"const logger = functionName => debug(`${process.env.PROJECT_NAME}:path:${functionName}`)"
		],
		"description": "Snippet Generator"
	},
	"Create first test": {
		"prefix": "ittrue",
		"body": [
			"it('should be true', () => expect(true).toBe(true));"
		],
		"description": "Create a truthy test to start"
	},
	"Arrange Act Assert Test": {
		"prefix": "aaaTest",
		"body": [
			"describe('${1}', () => {",
			"  it('should', () => {",
			"    //arrange",
			"    //act",
			"    //assert",
			"  });",
			"});"
		],
		"description": "Arrange Act Assert Test"
	},
	"Arrange Act Assert Comments": {
		"prefix": "aaa",
		"body": [
			"    //arrange",
			"    //act",
			"    //assert"
		],
		"description": "Arrange Act Assert Comments"
	}
}